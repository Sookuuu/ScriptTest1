local URL = "https://pastebin.com/raw/4YEBSYAq"

local function fetch(url)
    local req = (syn and syn.request) or http_request or request
    if req then
        local r = req({Url = url, Method = "GET"})
        if r and (r.Body or r.body) then
            return (r.Body or r.body)
        end
    end
    if game and game.HttpGet then
        return game:HttpGet(url, true)
    end
    error("No HTTP method available (executor must allow http).")
end

local src = fetch(URL)
assert(src and #src > 0, "Failed to fetch script.")
loadstring(src)()
